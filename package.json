{
  "name": "bgm-wiki-rev-diff",
  "description": "Build your UserScript with webpack",
  "version": "0.0.11",
  "private": true,
  "author": {
    "name": "Trim21",
    "email": "i@trim21.me"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src/**/*.js src/**/*.ts config/*.js && prettier --check **/*.yaml",
    "format": "eslint --fix src/**/*.js src/**/*.ts config/*.js && prettier --check --write **/*.yaml",
    "anylize": "cross-env npm_config_report=true npm run build",
    "postversion": "git push",
    "build": "webpack --config config/webpack.config.production.js",
    "dev": "webpack --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/bgm-wiki-rev-diff"
  },
  "dependencies": {
    "diff": "5.0.0",
    "diff2html": "3.1.15",
    "jquery": "3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/diff": "5.0.0",
    "@types/jquery": "3.5.4",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "cross-env": "7.0.2",
    "eslint": "7.13.0",
    "eslint-config-standard": "16.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "5.1.3",
    "prettier": "2.1.2",
    "terser-webpack-plugin": "5.1.1",
    "ts-loader": "8.0.11",
    "typescript": "4.2.3",
    "userscript-metadata-webpack-plugin": "0.0.6",
    "webpack": "5.26.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-livereload-plugin": "3.0.1",
    "webpack-merge": "5.4.0"
  }
}
