{
  "name": "bgm-wiki-rev-diff",
  "description": "Build your UserScript with webpack",
  "version": "0.0.16",
  "private": true,
  "author": {
    "name": "Trim21",
    "email": "i@trim21.me"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts src config && prettier --check **/*.yaml",
    "format": "eslint --fix src/**/*.js src/**/*.ts config/*.js && prettier --check --write **/*.yaml",
    "anylize": "cross-env npm_config_report=true npm run build",
    "postversion": "git push",
    "build": "webpack --config config/webpack.config.production.js",
    "dev": "webpack --config config/webpack.config.dev.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/bgm-wiki-rev-diff"
  },
  "dependencies": {
    "axios": "0.21.1",
    "diff": "5.0.0",
    "diff2html": "3.4.4",
    "jquery": "3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/diff": "5.0.0",
    "@types/jquery": "3.5.5",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "cross-env": "7.0.3",
    "eslint": "7.26.0",
    "eslint-config-standard": "16.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "6.0.0",
    "prettier": "2.3.0",
    "terser-webpack-plugin": "5.1.2",
    "ts-loader": "8.2.0",
    "typescript": "4.2.4",
    "userscript-metadata-webpack-plugin": "0.1.0",
    "webpack": "5.37.0",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "4.7.0",
    "webpack-livereload-plugin": "3.0.1",
    "webpack-merge": "5.7.3"
  }
}
